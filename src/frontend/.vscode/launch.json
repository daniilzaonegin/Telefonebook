{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "localhost (Chrome)",
      "url": "https://localhost:5002",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "disableNetworkCache": true,
      "sourceMapPathOverrides": {
          "webpack:///./*": "${webRoot}/*"
      }
    }
  ]
}